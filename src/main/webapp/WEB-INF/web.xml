<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.guide.servlet.gLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>    
	
	
	<servlet>
		<servlet-name>insert_guide</servlet-name>
		<servlet-class>com.guide.servlet.guideInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insert_guide</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>update_guide</servlet-name>
		<servlet-class>com.guide.servlet.guideUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>update_guide</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>delete_customer</servlet-name>
		<servlet-class>com.guide.servlet.guideDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete_customer</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	

	<servlet>
		<servlet-name>all_guide</servlet-name>
		<servlet-class>com.guide.servlet.selectAllGuide</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>all_guide</servlet-name>
		<url-pattern>/all</url-pattern>
	</servlet-mapping>
	
	











	
	
	<servlet>
		<servlet-name>all_travel</servlet-name>
		<servlet-class>com.traveler.servlet.allTravelerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>all_travel</servlet-name>
		<url-pattern>/all2</url-pattern>
	</servlet-mapping>
	
	
	
	
	<servlet>
		<servlet-name>update_travel</servlet-name>
		<servlet-class>com.traveler.servlet.updateTravelerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>update_travel</servlet-name>
		<url-pattern>/update2</url-pattern>
	</servlet-mapping>
	
	
	
	
	<servlet>
		<servlet-name>delete_travel</servlet-name>
		<servlet-class>com.traveler.servlet.deleteTravelerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete_travel</servlet-name>
		<url-pattern>/delete2</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>insert_travel</servlet-name>
		<servlet-class>com.traveler.servlet.insertTravelerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insert_travel</servlet-name>
		<url-pattern>/insert2</url-pattern>
	</servlet-mapping>
	
	
	
	
	

</web-app>